{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "github>Kotori316/common-script:renovate-default",
    ":rebaseStalePrs"
  ],
  "packageRules": [
    {
      "matchManagers": [
        "gradle"
      ],
      "matchPackagePatterns": [
        "^mezz.jei:*"
      ],
      "automerge": true
    },
    {
      "matchManagers": [
        "gradle"
      ],
      "matchPackagePatterns": [
        "^com.electronwill.night-config:*"
      ],
      "enabled": false
    },
    {
      "matchDatasources": [
        "docker"
      ],
      "matchPackageNames": [
        "ghcr.io/kotori316/quarry-cache"
      ],
      "enabled": false
    }
  ]
}
