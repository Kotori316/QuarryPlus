{
  "multipart": [
    {"apply": {"model": "quarryplus:block/frame_post"}},
    {
      "when": {"north": "true"},
      "apply": {"model": "quarryplus:block/frame_side", "uvlock": true}
    },
    {
      "when": {"east": "true"},
      "apply": {"model": "quarryplus:block/frame_side", "y": 90, "uvlock": true}
    },
    {
      "when": {"south": "true"},
      "apply": {"model": "quarryplus:block/frame_side", "y": 180, "uvlock": true}
    },
    {
      "when": {"west": "true"},
      "apply": {"model": "quarryplus:block/frame_side", "y": 270, "uvlock": true}
    },
    {
      "when": {"up": "true"},
      "apply": {"model": "quarryplus:block/frame_side", "x": 270, "uvlock": true}
    },
    {
      "when": {"down": "true"},
      "apply": {"model": "quarryplus:block/frame_side", "x": 90, "uvlock": true}
    }
  ]
}
